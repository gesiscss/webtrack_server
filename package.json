{
  "name": "Webtrack-Server",
  "author": "Universität Koblenz · Landau",
  "keywords": [
    "tracking",
    "web-tracking"
  ],
  "version": "1.0.0",
  "private": true,
  "main": "/bin/www",
  "scripts": {
    "maxprod": "node --max-old-space-size=53248 ./bin/www.js",
    "prod": "node --max-old-space-size=24576 ./bin/www.js",
    "start": "node --max-old-space-size=8192 ./bin/www.js",
    "light": "node --max-old-space-size=2048 ./bin/www.js",
    "forever_maxprod": "forever start -a -l ~/webtrack_server/storage/logs/forever.log -e ~/webtrack_server/storage/logs/forever_error.log -o ~/webtrack_server/storage/logs/forever_out.log --max-old-space-size=53248 ./bin/www.js",
    "forever_prod": "forever start -a -l ~/webtrack_server/storage/logs/forever.log -e ~/webtrack_server/storage/logs/forever_error.log -o ~/webtrack_server/storage/logs/forever_out.log --max-old-space-size=24576 ./bin/www.js",
    "forever_start": "forever start -a -l ~/webtrack_server/storage/logs/forever.log -e ~/webtrack_server/storage/logs/forever_error.log -o ~/webtrack_server/storage/logs/forever_out.log --max-old-space-size=8192 ./bin/www.js",
    "forever_light": "forever start -a -l ~/webtrack_server/storage/logs/forever.log -e ~/webtrack_server/storage/logs/forever_error.log -o ~/webtrack_server/storage/logs/forever_out.log --max-old-space-size=512 ./bin/www.js",
    "debug": "nodemon --inspect --max-old-space-size=2048 ./bin/www  APPDEBUG=1"
  },
  "dependencies": {
    "bee-queue": "^1.2.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.3",
    "express": "^4.19.2",
    "express-json-validator-middleware": "^1.2.3",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^5.2.0",
    "file-db": "0.0.2",
    "forever": "^2.0.0",
    "helmet": "^3.21.1",
    "html-minifier": "^3.5.17",
    "jsonwebtoken": "^8.2.1",
    "log-timestamp": "^0.3.0",
    "log-to-file": "^1.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "mysql": "^2.15.0",
    "navigator": "^1.0.1",
    "node-cache": "^4.2.0",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.2",
    "openssl-wrapper": "^0.3.4",
    "rate-limit-redis": "^1.7.0",
    "redis": "^2.8.0",
    "rimraf": "^2.6.3",
    "schema-inspector": "^1.6.8",
    "serve-favicon": "~2.4.2",
    "stream-buffers": "^3.0.2",
    "unzip": "^0.1.11",
    "valid-url": "^1.0.9",
    "window": "^4.2.5"
  }
}
